{
    "custom/quicklink1": {
        "format": " ",
        "on-click": "~/.config/ml4w/apps/ML4W_Hyprland_Settings-x86_64.AppImage",
        "tooltip-format": "Open Hyprland Settings"
    }, 
    "custom/quicklink2": {
        "format": " ",
        "on-click": "~/.config/ml4w/settings/browser.sh",
        "tooltip-format": "Open the browser"
    },
    "custom/quicklink3": {
        "format": " ",
        "on-click": "~/.config/ml4w/settings/filemanager.sh",
        "tooltip-format": "Open the filemanager"
    }, 
    "custom/quicklink4": {
        "format": " ",
        "on-click": "~/.config/ml4w/settings/vscode.sh",
        "tooltip-format": "Open Visual Studio Code"
    }, 
    "custom/quicklink5": {
        "format": " ", //        
        "on-click": "~/.config/ml4w/settings/github-desktop.sh",
        "tooltip-format": "Open GitHub-Desktop"
    }, 
    //  Gnome Software       
    "custom/quicklink6": {
        "format": " ", //        
        "on-click": "gnome-software",
        "tooltip-format": "Gnome Software"
    }, 
    "custom/quicklinkempty": {
    }, 
    "group/quicklinks": {
        "orientation": "horizontal",
        "modules": [
            "custom/quicklink1",
            "custom/quicklink2",
            "custom/quicklink3",
            "custom/quicklink4",
            "custom/quicklink5",
            "custom/quicklink6",
            "custom/quicklinkempty"
        ]
    }
}
